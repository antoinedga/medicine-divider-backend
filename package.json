{
  "name": "medicine-divider-backend",
  "version": "0.0.1",
  "private": true,
  "description": "Medical Record sharing and medicine routine hub",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "NODE_ENV=dev DEBUG=* nodemon app.js",
    "test": "NODE_ENV=test DEBUG=* jest --coverage",
    "eslint": "eslint src --config .eslintrc.js"
  },
  "dependencies": {
    "auth0": "^4.3.1",
    "body-parser": "^1.20.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "express-openid-connect": "^2.17.1",
    "express-rate-limit": "^7.1.3",
    "express-request-id": "^3.0.0",
    "express-validator": "^7.0.1",
    "handlebars": "^4.7.8",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "moment": "^2.30.1",
    "mongoose": "^7.5.3",
    "morgan": "~1.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "validator": "^13.11.0",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.1.1",
    "eslint": "^9.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^3.0.0",
    "globals": "^15.1.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "mongodb-memory-server": "^9.3.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/models/*",
      "!src/configs/*"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "html",
      "text",
      "lcov",
      "json"
    ],
    "setupFilesAfterEnv": [
      "./test/src/jest.setup.js"
    ]
  }
}
